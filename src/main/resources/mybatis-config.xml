<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
        <setting name="callSettersOnNulls" value="true"/>
        <setting name="jdbcTypeForNull" value="NULL"/>
    </settings>

	<typeAliases>
		<typeAlias alias="CommonCode" type="kr.co.strato.mcmp.common.model.CommonCode"/>
		<!-- typeAliases 를 package로 등록할 경우 해당 패키지 이하의 클래스가 등록된다-->
		<package name="kr.co.strato.mcmp.argocd.model"/>
		<package name="kr.co.strato.mcmp.catalog.model"/>
		<package name="kr.co.strato.mcmp.deploy.model"/>
		<package name="kr.co.strato.mcmp.jenkins.pipeline.model"/>
		<package name="kr.co.strato.mcmp.k8s.model"/>
		<package name="kr.co.strato.mcmp.oss.model"/>
		<package name="kr.co.strato.mcmp.workflow.model"/>
    </typeAliases>
</configuration>